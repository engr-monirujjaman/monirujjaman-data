<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>Monirujjaman.Data</PackageId>
        <Version>1.0.0</Version>
        <Authors>Monirujjaman</Authors>
        <Company>Monirujjaman</Company>
        <Product>Monirujjaman.Data</Product>
        <Description>This is a repository pattern library</Description>
        <PackageIcon>md_monir.png</PackageIcon>
        <RepositoryUrl>https://github.com/engr-monirujjaman/monirujjaman-data/tree/master</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>repository pattern, data</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <AnalysisLevel>6.0</AnalysisLevel>
        <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
        <NeutralLanguage>en-US</NeutralLanguage>
        <GenerateDocumentationFile>False</GenerateDocumentationFile>
        <SignAssembly>False</SignAssembly>
        <AssemblyName>Monirujjaman.Data</AssemblyName>
        <RootNamespace>Monirujjaman.Data</RootNamespace>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
      <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
      <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\..\..\..\Downloads\md_monir.png">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Data.SqlClient" Version="4.1.0" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.2" />
      <PackageReference Include="System.Linq.Dynamic.Core" Version="1.2.17" />
      <PackageReference Include="Z.EntityFramework.Extensions.EFCore" Version="6.13.12" />
        <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.2.2" />
        <PackageReference Include="Microsoft.AspNetCore.Http.Features" Version="5.0.14" />
    </ItemGroup>

</Project>
